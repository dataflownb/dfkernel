{
  "name": "@dfnotebook/dfnotebook-extension",
  "version": "0.1.0-beta.0",
  "description": "A JupyterLab extension for running dataflow notebooks",
  "main": "index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "author": {
    "name": "Dataflow Notebook Development Team",
    "email": "dataflownb@users.noreply.github.com"
  },
  "homepage": "https://github.com/dataflownb/dfnotebook-extension",
  "license": "BSD-3-Clause",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build",
    "build:prod": "lerna run build:prod",
    "build:dev": "lerna run build:dev",
    "clean": "lerna run clean",
    "clean:slate": "python clean.py",
    "develop": "jupyter labextension develop . --overwrite",
    "install": "lerna bootstrap",
    "install:extension": "jlpm build",
    "watch": "run-p watch:cells watch:notebook watch:outputarea watch:extension",
    "watch:cells": "lerna exec --stream --scope \"@dfnotebook/dfcells\" jlpm watch",
    "watch:notebook": "lerna exec --stream --scope \"@dfnotebook/dfnotebook\" jlpm watch",
    "watch:outputarea": "lerna exec --stream --scope \"@dfnotebook/dfoutputarea\" jlpm watch",
    "watch:extension": "lerna exec --stream --scope \"@dfnotebook/dfnotebook-extension\" jlpm watch"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.4.2",
    "@jupyterlab/buildutils": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "extra-watch-webpack-plugin": "^1.0.3",
    "husky": "^3",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-raw-loader": "^1.0.1",
    "jest-summary-reporter": "^0.0.2",
    "lerna": "^3.22.1",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "rimraf": "^3.0.2",
    "shell-quote": "^1.7.2",
    "typescript": "~4.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "dfnotebook-extension/labextension",
    "disabledExtensions": ["@jupyterlab/notebook-extension"]
  }
}
